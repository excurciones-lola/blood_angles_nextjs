{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "react"], // Add react plugin
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": ["warn"],
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": "error"
  }
}
